            "vulnerability": {
                "@id": "https://ubuntu.com/security/notices/USN-4128-1",
                "name": "USN-4128-1",
                "description": "It was discovered that the Tomcat 8 SSI printenv command echoed user  provided data without escaping it. An attacker could possibly use this  issue to perform an XSS attack. (CVE-2019-0221)  It was discovered that Tomcat 8 did not address HTTP/2 connection window exhaustion on write while addressing CVE-2019-0199. An attacker could  possibly use this issue to cause a denial of service. (CVE-2019-10072)",
                "aliases": [
                    "CVE-2019-0221",
                    "CVE-2019-10072"
                ]
            },
