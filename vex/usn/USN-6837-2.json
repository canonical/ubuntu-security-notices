            "vulnerability": {
                "@id": "https://ubuntu.com/security/notices/USN-6837-2",
                "name": "USN-6837-2",
                "description": "It was discovered that Rack incorrectly parsed certain media types. A remote attacker could possibly use this issue to cause Rack to consume resources, leading to a denial of service. This issue only affected  Ubuntu 20.04 LTS and Ubuntu 22.04 LTS. (CVE-2024-25126)  It was discovered that Rack incorrectly handled certain Range headers. A remote attacker could possibly use this issue to cause Rack to create  large responses, leading to a denial of service. (CVE-2024-26141)  It was discovered that Rack incorrectly handled certain crafted headers. A remote attacker could possibly use this issue to cause Rack to consume resources, leading to a denial of service. (CVE-2024-26146)",
                "aliases": [
                    "CVE-2024-25126",
                    "CVE-2024-26141",
                    "CVE-2024-26146"
                ]
            },
