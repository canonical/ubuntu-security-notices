            "vulnerability": {
                "@id": "https://ubuntu.com/security/notices/USN-6594-1",
                "name": "USN-6594-1",
                "description": "Joshua Rogers discovered that Squid incorrectly handled HTTP message processing. A remote attacker could possibly use this issue to cause Squid to crash, resulting in a denial of service. (CVE-2023-49285)  Joshua Rogers discovered that Squid incorrectly handled Helper process management. A remote attacker could possibly use this issue to cause Squid to crash, resulting in a denial of service. (CVE-2023-49286)  Joshua Rogers discovered that Squid incorrectly handled HTTP request parsing. A remote attacker could possibly use this issue to cause Squid to crash, resulting in a denial of service. (CVE-2023-50269)",
                "aliases": [
                    "CVE-2023-49285",
                    "CVE-2023-49286",
                    "CVE-2023-50269"
                ]
            },
