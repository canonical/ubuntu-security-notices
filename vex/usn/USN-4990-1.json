            "vulnerability": {
                "@id": "https://ubuntu.com/security/notices/USN-4990-1",
                "name": "USN-4990-1",
                "description": "It was discovered that Nettle incorrectly handled RSA decryption. A remote attacker could possibly use this issue to cause Nettle to crash, resulting in a denial of service. (CVE-2021-3580)  It was discovered that Nettle incorrectly handled certain padding oracles. A remote attacker could possibly use this issue to perform a variant of the Bleichenbacher attack. This issue only affected Ubuntu 18.04 LTS. (CVE-2018-16869)",
                "aliases": [
                    "CVE-2021-3580"
                ]
            },
