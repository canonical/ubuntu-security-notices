            "vulnerability": {
                "@id": "https://ubuntu.com/security/notices/USN-6436-1",
                "name": "USN-6436-1",
                "description": "It was discovered that the FRR did not properly check the attribute length in NRLI. A remote attacker could possibly use this issue to cause a denial of service. (CVE-2023-41358)  It was discovered that the FRR did not properly manage memory when reading initial bytes of ORF header. A remote attacker could possibly use this issue to cause a denial of service. (CVE-2023-41360)  It was discovered that FRR did not properly validate the attributes in the BGP FlowSpec functionality. A remote attacker could possibly use this issue to cause a denial of service. (CVE-2023-41909)",
                "aliases": [
                    "CVE-2023-41358",
                    "CVE-2023-41360",
                    "CVE-2023-41909"
                ]
            },
