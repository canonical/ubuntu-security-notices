            "vulnerability": {
                "@id": "https://ubuntu.com/security/notices/USN-6166-1",
                "name": "USN-6166-1",
                "description": "David Gstir discovered that libcap2 incorrectly handled certain return codes. An attacker could possibly use this issue to cause libcap2 to consume memory, leading to a denial of service. (CVE-2023-2602)  Richard Weinberger discovered that libcap2 incorrectly handled certain long input strings. An attacker could use this issue to cause libcap2 to crash, resulting in a denial of service, or possibly execute arbitrary code. (CVE-2023-2603)",
                "aliases": [
                    "CVE-2023-2602",
                    "CVE-2023-2603"
                ]
            },
