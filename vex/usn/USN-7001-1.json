            "vulnerability": {
                "@id": "https://ubuntu.com/security/notices/USN-7001-1",
                "name": "USN-7001-1",
                "description": "Shang-Hung Wan discovered that Expat, contained within the xmltok library, did not properly handle certain function calls when a negative input length was provided. An attacker could use this issue to cause a denial of service or possibly execute arbitrary code. (CVE-2024-45490)  Shang-Hung Wan discovered that Expat, contained within the xmltok library, did not properly handle the potential for an integer overflow on 32-bit platforms. An attacker could use this issue to cause a denial of service or possibly execute arbitrary code. (CVE-2024-45491)",
                "aliases": [
                    "CVE-2024-45490",
                    "CVE-2024-45491"
                ]
            },
