            "vulnerability": {
                "@id": "https://ubuntu.com/security/notices/USN-6126-1",
                "name": "USN-6126-1",
                "description": "It was discovered that libvirt incorrectly handled the nwfilter driver. A local attacker could possibly use this issue to cause libvirt to crash, resulting in a denial of service. This issue only affected Ubuntu 22.04 LTS. (CVE-2022-0897)  It was discovered that libvirt incorrectly handled queries for the SR-IOV PCI device capabilities. A local attacker could possibly use this issue to cause libvirt to consume resources, leading to a denial of service. (CVE-2023-2700)",
                "aliases": [
                    "CVE-2022-0897",
                    "CVE-2023-2700"
                ]
            },
