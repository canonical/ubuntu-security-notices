{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://raw.githubusercontent.com/canonical/ubuntu-security-notices/main/usn/schema.json",
  "title": "Ubuntu Security Notice",
  "description": "A schema for describing a a security update in Ubuntu.",
  "type": "object",
  "properties": {
    "schema_version": {
      "type": "string"
    },
    "id": {
      "type": "string"
    },
    "action": {
      "type": "string"
    },
    "cves": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "description": {
      "type": "string"
    },
    "isummary": {
      "type": "string"
    },
    "releases": {
      "type": "object"
    },
    "summary": {
      "type": "string"
    },
    "timestamp": {
      "type": "number"
    },
    "title": {
      "type": "string"
    }
  },
  "required": [
    "action",
    "cves",
    "description",
    "id",
    "isummary",
    "releases",
    "summary",
    "timestamp",
    "title"
  ],
  "additionalProperties": false
}
