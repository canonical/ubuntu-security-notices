            "vulnerability": {
                "@id": "https://ubuntu.com/security/notices/USN-2817-1",
                "name": "USN-2817-1",
                "description": "It was discovered that IcedTea Web incorrectly handled applet URLs. A remote attacker could possibly use this issue to inject applets into the .appletTrustSettings configuration file and bypass user approval. (CVE-2015-5234)  Andrea Palazzo discovered that IcedTea Web incorrectly determined the origin of unsigned applets. A remote attacker could possibly use this issue to bypass user approval, or to trick the user into approving applet execution. (CVE-2015-5235)",
                "aliases": [
                    "CVE-2015-5234",
                    "CVE-2015-5235"
                ]
            },
